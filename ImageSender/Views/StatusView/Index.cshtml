@model IEnumerable<ImageSender.Models.FolderFiles>

<div class="container">
    <h2>Email Status</h2>


    <div class="row table-responsive">
        <div class="col-md-12">
            <table class="table table-striped table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th>Id</th>
                        <th>Email ID</th>
                        <th>Folder Path</th>
                        <th style="width: 105px;">Email Sent</th>
                    </tr>
                </thead>


                <tbody class="">
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Id</td>
                            <td>@item.EmailID</td>
                            <td>@item.FolderPath</td>
                            <td align="center">@(item.IsMailSend ? "Yes" : "No")</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
